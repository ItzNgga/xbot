{  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": "./",
    "baseUrl": "./",
    "outDir": "dist",
    "target": "ES2020",
    "noEmitOnError": false,
    "esModuleInterop": true,
    "sourceMap": false,
    "skipLibCheck": true,
    "noImplicitReturns": false,
    "typeRoots": ["./node_modules/@types", "./types"]
  },
  "include": [
    "src/**/*.ts",
    "index.ts",
    "types**/*.ts",
    "nsfw_model"
  ],
  "exclude": ["temp", "sticker", "json", "image", "groups", "gif", "fonts", "cloud"]
}
