{  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "dist",
    "noEmitOnError": false,
    "esModuleInterop": true,
    "sourceMap": false,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*.ts",
    "index.ts",
    "types**/*.ts"
  ],
  "exclude": ["temp", "sticker", "json", "image", "groups","gif","fonts","cloud"]
}
